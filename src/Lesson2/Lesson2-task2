public class Main {
    public static void main(String[] args) {
        int a = 5;
        int b = 10;
        checkSumSign(a, b);
    }
    static void checkSumSign(int x, int y) {
        int z = x + y;
        if (z >= 0) {
            System.out.println("Сумма положительная");
        } else {
            System.out.println("Сумма отрицательная");
        }
    }
}