public class Main {
    public static void main(String[] args) {
        positiveOrNegative();
    }
    private static void positiveOrNegative() {
        int a = 5;
        if (a >= 0) {
            System.out.println("Число " + a + " положительное");
        } else {
            System.out.println("Число " + a + " отрицательное");
        }
    }
}